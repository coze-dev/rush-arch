{
  "name": "@coze-arch/rush-increment-run-plugin",
  "version": "0.0.2",
  "description": "incremently run commands for ci",
  "license": "ISC",
  "author": "tecvan.fe@gmail.com",
  "maintainers": [],
  "main": "src/index.ts",
  "bin": {
    "rush-increment-run": "src/debug.js"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo",
    "!**/*.map",
    "command-line.json",
    "rush-plugin-manifest.json"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint ./ --cache",
    "test": "vitest --run --passWithNoTests",
    "test:cov": "npm run test -- --coverage"
  },
  "dependencies": {
    "@actions/core": "~1.11.1",
    "lodash": "^4.17.21",
    "shelljs": "^0.9.2"
  },
  "devDependencies": {
    "@coze-arch/build-lib-preset": "workspace:*",
    "@coze-arch/eslint-config": "workspace:*",
    "@coze-arch/fs-enhance": "workspace:*",
    "@coze-arch/logger": "workspace:*",
    "@coze-arch/monorepo-kits": "workspace:*",
    "@coze-arch/package-audit": "workspace:*",
    "@coze-arch/ts-config": "workspace:*",
    "@coze-arch/vitest-config": "workspace:*",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22",
    "@types/shelljs": "^0.8.15",
    "@vitest/coverage-v8": "^3.0.9",
    "commander": "^13.1.0",
    "rollup": "^4.41.1",
    "stylelint": "^15.11.0",
    "sucrase": "^3.32.0",
    "vitest": "^3.0.9"
  },
  "peerDependencies": {
    "commander": "^13.1.0",
    "stylelint": "^15.11.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "cozePublishConfig": {
    "main": "lib/index.js",
    "bin": {
      "rush-increment-run": "lib/index.js"
    }
  }
}
